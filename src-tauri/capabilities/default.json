{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "shell:allow-open",
    "log:default",
    "updater:allow-check",
    "updater:allow-download",
    "updater:allow-download-and-install",
    "updater:allow-install",
    "dialog:allow-open",
    "dialog:default",
    "fs:allow-exists",
    "fs:allow-read-dir",
    "fs:allow-read-file",
    "core:window:allow-start-dragging"
  ]
}